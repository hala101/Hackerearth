#Given three integers (x, y, andz ) representing the three sides of a triangle, identify whether the triangle is Scalene, Isosceles, or Equilateral.

read x;
read y;
read z;
if [ $x -eq $y ] && [ $y -eq $z ]
then
echo "EQUILATERAL";
elif [ $x -ne $y ] && [ $x -ne $z ] && [ $y -ne $z ]
then
echo "SCALENE";
else
echo "ISOSCELES";
fi
